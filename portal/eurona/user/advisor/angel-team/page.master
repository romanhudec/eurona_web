								<%} %>

<%--								<%if ( ( this.LogedAdvisor != null && ( this.LogedAdvisor.AngelTeamClen || this.LogedAdvisor.AngelTeamManager ) || (Eurona.Security.IsLogged(false) && Eurona.Security.IsInRole(Eurona.DAL.Entities.Role.ADMINISTRATOR)) ) ){ %>
